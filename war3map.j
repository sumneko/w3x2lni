//Generated allocator of Cooldown
function s__Cooldown__allocate takes nothing returns integer
 local integer this=si__Cooldown_F
    if (this!=0) then
        set si__Cooldown_F=si__Cooldown_V[this]
    else
        set si__Cooldown_I=si__Cooldown_I+1
        set this=si__Cooldown_I
    endif
    if (this>8190) then
        call DisplayTimedTextToPlayer(GetLocalPlayer(),0,0,1000.,"Unable to allocate id for an object of type: Cooldown")
        return 0
    endif

    set si__Cooldown_V[this]=-1
 return this
endfunction

//Generated destructor of Cooldown
function s__Cooldown_deallocate takes integer this returns nothing
    if this==null then
            call DisplayTimedTextToPlayer(GetLocalPlayer(),0,0,1000.,"Attempt to destroy a null struct of type: Cooldown")
        return
    elseif (si__Cooldown_V[this]!=-1) then
            call DisplayTimedTextToPlayer(GetLocalPlayer(),0,0,1000.,"Double free of type: Cooldown")
        return
    endif
    set si__Cooldown_V[this]=si__Cooldown_F
    set si__Cooldown_F=this
endfunction

