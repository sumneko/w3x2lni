//library IsUnitSpelling:
    function s__Cooldown_create takes unit un,integer i returns integer
        local integer a=s__Cooldown__allocate()
        set s__Cooldown_u[a]=un
        set s__Cooldown_id[a]=i
        return a
    endfunction
    function s__Cooldown_destroy takes integer this returns nothing
        local integer l=GetUnitAbilityLevel(s__Cooldown_u[this], s__Cooldown_id[this])
        call UnitRemoveAbility(s__Cooldown_u[this], s__Cooldown_id[this])
        call UnitAddAbility(s__Cooldown_u[this], s__Cooldown_id[this])
        call SetUnitAbilityLevel(s__Cooldown_u[this], s__Cooldown_id[this], l)
        call SetUnitAnimation(s__Cooldown_u[this], "stand")
        set s__Cooldown_u[this]=null
        set s__Cooldown_id[this]=0
    endfunction
function IsUnitSpelling takes unit u returns boolean
    return IsUnitInGroup(u, IsUnitSpelling__g)
endfunction
function AbilityResetCooldown takes unit u,integer abilId,boolean wait returns nothing
    local integer l=GetUnitAbilityLevel(u, abilId)
    local integer id
    local integer i=s__Cooldown_create(u , abilId)
    if GetUnitAbilityLevel(u, abilId) > 0 then
        if wait == false then
            call s__Cooldown_destroy(i)
        else
            if IsUnitSpelling(u) == false then
                call s__Cooldown_destroy(i)
            endif
        endif
    endif
endfunction
function IsUnitSpelling__SpellingGroupAddUnit takes nothing returns nothing
    call GroupAddUnit(IsUnitSpelling__g, GetTriggerUnit())
endfunction
function IsUnitSpelling__SpellingGroupRemoveUnit takes nothing returns nothing
    local integer i=0
    call GroupRemoveUnit(IsUnitSpelling__g, GetTriggerUnit())
    loop
        exitwhen i == si__Cooldown_I
        if s__Cooldown_u[i] == GetTriggerUnit() then
            call s__Cooldown_destroy(i)
            exitwhen true
        endif
        set i=i + 1
    endloop
endfunction
function IsUnitSpelling__SpellingEvent takes nothing returns nothing
    local trigger t1=CreateTrigger()
    local trigger t2=CreateTrigger()
    local integer i=0
    loop
        call TriggerRegisterPlayerUnitEvent(t1, Player(i), EVENT_PLAYER_UNIT_SPELL_ENDCAST, null)
        call TriggerRegisterPlayerUnitEvent(t2, Player(i), EVENT_PLAYER_UNIT_SPELL_CHANNEL, null)
        set i=i + 1
        exitwhen i == 15
    endloop
    call TriggerAddAction(t1, function IsUnitSpelling__SpellingGroupRemoveUnit)
    call TriggerAddAction(t2, function IsUnitSpelling__SpellingGroupAddUnit)
endfunction

