        set redc=( YDWEAdvancedUnitDataSystem__DAMAGE_TEST - test + GetWidgetLife(u) ) / YDWEAdvancedUnitDataSystem__DAMAGE_TEST
        if enab then
            call EnableTrigger(trig)
        endif
        call UnitRemoveAbility(u, aid)
        call SetWidgetLife(u, life)
        set trig=null
        if redc >= 1. then
            return YDWEAdvancedUnitDataSystem__ARMOR_INVULNERABLE
        elseif redc < 0. then
            return - YDWEAdvancedUnitDataSystem__Log(redc + 1.) / YDWEAdvancedUnitDataSystem__NATLOG_094
        else
            return redc / ( YDWEAdvancedUnitDataSystem__ARMOR_REDUCTION_MULTIPLIER1 * ( 1. - redc ) )
        endif
    endif
    set trig=null
    return 0.
endfunction

